{
  "name": "api",
  "version": "1.0.0",
  "description": "API",
  "keywords": [
    "API"
  ],
  "author": "Xi",
  "license": "MIT",
  "homepage": "https://github.com/xiarx/api#readme",
  "bugs": {
    "url": "https://github.com/xiarx/api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiarx/api.git"
  },
  "engines": {
    "node": ">= 22.2.0",
    "npm": ">= 10.7.0"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node lib/index.js",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "test",
    "style": "prettier . --write -u",
    "lint": "eslint src",
    "type": "tsc --noEmit",
    "migrate": "npx prisma migrate dev",
    "compose": "docker-compose up",
    "docker": "docker build -t api . && docker run -p 80:4000 --env-file .env -d --name api api"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/compression": "^1.7.5",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "eslint": "^8.56.0",
    "prettier": "^3.3.1",
    "prisma": "^5.15.0",
    "tsx": "^4.15.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  }
}
